<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=Edge">
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <title>alenen.com | Yönetim </title>

    <!-- Favicon-->
    <link rel="icon" href="favicon.ico" type="~/Areas/AreaAdmin/AdminContent/images/x-icon">
    <link rel="SHORTCUT ICON" href="~/Areas/AreaAdmin/AdminContent/favicon.ico">
    <!-- Google Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Roboto:400,700&subset=latin,cyrillic-ext" rel="stylesheet" type="text/css">
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet" type="text/css">
    <!-- Bootstrap Core Css -->
    <link href="~/Areas/AreaAdmin/AdminContent/plugins/bootstrap/css/bootstrap.css" rel="stylesheet">
    <!-- Waves Effect Css -->
    <link href="~/Areas/AreaAdmin/AdminContent/plugins/node-waves/waves.css" rel="stylesheet" />
    <!-- Animation Css -->
    <link href="~/Areas/AreaAdmin/AdminContent/plugins/animate-css/animate.css" rel="stylesheet" />
    <!-- Morris Chart Css-->
    <link href="~/Areas/AreaAdmin/AdminContent/plugins/morrisjs/morris.css" rel="stylesheet" />
    <!-- Custom Css -->
    <link href="~/Areas/AreaAdmin/AdminContent/css/style.css" rel="stylesheet">
    
    <link href="~/Areas/AreaAdmin/AdminContent/css/themes/all-themes.css" rel="stylesheet" />
    <script src="~/Areas/AreaAdmin/AdminContent/plugins/bootstrap-select/js/bootstrap-select.js"></script>
    <script src="~/Areas/AreaAdmin/AdminContent/plugins/bootstrap-select/js/bootstrap-select.min.js"></script>
    <link href="~/Areas/AreaAdmin/AdminContent/plugins/bootstrap-select/css/bootstrap-select.css" rel="stylesheet" />
    <link href="~/Areas/AreaAdmin/AdminContent/plugins/bootstrap-select/css/bootstrap-select.min.css" rel="stylesheet" />
   
</head>

<body class="theme-green">
    <!-- Page Loader -->
    <div class="page-loader-wrapper">
        <div class="loader">
            <div class="preloader">
                <div class="spinner-layer pl-red">
                    <div class="circle-clipper left">
                        <div class="circle"></div>
                    </div>
                    <div class="circle-clipper right">
                        <div class="circle"></div>
                    </div>
                </div>
            </div>
            <p>Sayfa Yükleniyor...</p>
        </div>
    </div>
    <!-- #END# Page Loader -->
    <!-- Overlay For Sidebars -->
    <div class="overlay"></div>
    <!-- #END# Overlay For Sidebars -->
    <!-- Search Bar -->
    <div class="search-bar">
        <div class="search-icon">
            <i class="material-icons">search</i>
        </div>
        <input type="text" placeholder="START TYPING...">
        <div class="close-search">
            <i class="material-icons">close</i>
        </div>
    </div>
    <!-- #END# Search Bar -->
    <!-- Top Bar -->
    @{
        var user = Session["LoginAdminUser"] as alenen.User;
    }

    <nav class="navbar">
        <div class="container-fluid">
            <div class="navbar-header">
                <a href="javascript:void(0);" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar-collapse" aria-expanded="false"></a>
                <a href="javascript:void(0);" class="bars"></a>
                <a class="navbar-brand" href="/">Alenen.com | Yönetim</a>
            </div>
            <div class="collapse navbar-collapse" id="navbar-collapse">
                <ul class="nav navbar-nav navbar-right">

                    <!-- Notifications -->
                    <!-- Tasks -->
                    <!-- #END# Tasks -->

                </ul>
            </div>
        </div>
    </nav>
    <!-- #Top Bar -->
    <section>
        <!-- Left Sidebar -->
        <aside id="leftsidebar" class="sidebar">
            <!-- User Info -->
            <div class="user-info">

                <div class="info-container">
                    <div class="name" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                        <img src="~/Content/images/uploads/kullaniciresim/@user.resim"style="width:50px; height:auto;" /><br />

                        Sayın @user.isim @user.soyisim <br />
                        Hoş Gelginiz



                    </div>

                    <div class="btn-group user-helper-dropdown">
                        <i class="material-icons" data-toggle="dropdown" aria-haspopup="true" aria-expanded="true">keyboard_arrow_down</i>
                        <ul class="dropdown-menu pull-right">

                            <li><a href="/LogOut"><i class="material-icons">input</i>Çıkış</a></li>
                        </ul>
                    </div>
                </div>
            </div>
            <!-- #User Info -->
            <!-- Menu -->
            <div class="menu">
                <ul class="list">
                    <li class="header">İŞLEM MENÜSÜ</li>


                    <li>
                        <a href="/AreaAdmin/Default">
                            <i class="material-icons">home</i>
                            <span>Anasayfa</span>
                        </a>
                    </li>

                    <li>

                        <a href="javascript:void(0);" class="menu-toggle">
                            <i class="material-icons">person</i>
                            <span>Admin - Modaretör İşlemleri</span>
                        </a>
                        <ul class="ml-menu">
                            <li>

                                <a href="/AreaAdmin/Yorumİslemleri/Onaybekleyenyorum">Onay Bekleyen Yorumlar</a>
                            </li>
                            <li>
                                <a href="/AreaAdmin/Users/Onaybekleyenkullanici">Onay Bekleyen Kullanıcılar</a>
                            </li>


                        </ul>
                    </li>
                    <li>
                        <a href="/AreaAdmin/AnaKategori">
                            <i class="material-icons">category</i>
                            <span>Ana Kategorileri Yönet</span>
                        </a>
                    </li>

                    <li>
                        <a href="/AreaAdmin/Kategoriİslemleri">
                            <i class="material-icons">category</i>
                            <span>Kategorileri Yönet</span>
                        </a>
                    </li>

                    <li>
                        <a href="/AreaAdmin/Konuİslemleri">
                            <i class="material-icons">subject</i>
                            <span>Konuları Yönet</span>
                        </a>
                    </li>


                    <li>
                        <a href="/AreaAdmin/Yorumİslemleri">
                            <i class="material-icons">comment</i>
                            <span>Yorumları Yönet</span>
                        </a>
                    </li>
                    <li>
                        <a href="/AreaAdmin/Users">
                            <i class="material-icons">account_box</i>
                            <span>Üyeleri Yönet</span>
                        </a>
                    </li>
                </ul>
            </div>
          
            <!-- #Menu -->
            <!-- Footer -->
            <div class="legal">
                <div class="copyright">
                    &copy; @DateTime.Now.Year<a href="javascript:void(0);"> Created By Nist Group</a>.
                </div>
                <div class="version">
                    <b>Version: </b> 1.0.0
                </div>
            </div>
            <!-- #Footer -->
        </aside>
        <!-- #END# Left Sidebar -->
        <!-- Right Sidebar -->
        <!-- #END# Right Sidebar -->
    </section>
    <section class="content">
        @RenderBody()
    </section>
</body>
</html>
<!-- Jquery Core Js -->
<script src="~/Areas/AreaAdmin/AdminContent/plugins/jquery/jquery.min.js"></script>
<!-- Bootstrap Core Js -->
<script src="~/Areas/AreaAdmin/AdminContent/plugins/bootstrap/js/bootstrap.js"></script>
<!-- Select Plugin Js -->
<script src="~/Areas/AreaAdmin/AdminContent/plugins/bootstrap-select/js/bootstrap-select.js"></script>
<!-- Slimscroll Plugin Js -->
<script src="~/Areas/AreaAdmin/AdminContent/plugins/jquery-slimscroll/jquery.slimscroll.js"></script>
<!-- Waves Effect Plugin Js -->
<script src="~/Areas/AreaAdmin/AdminContent/plugins/node-waves/waves.js"></script>
<!-- Jquery CountTo Plugin Js -->
<script src="~/Areas/AreaAdmin/AdminContent/plugins/jquery-countto/jquery.countTo.js"></script>
<!-- Morris Plugin Js -->
<script src="~/Areas/AreaAdmin/AdminContent/plugins/raphael/raphael.min.js"></script>
<script src="~/Areas/AreaAdmin/AdminContent/plugins/morrisjs/morris.js"></script>
<!-- ChartJs -->
<script src="~/Areas/AreaAdmin/AdminContent/plugins/chartjs/Chart.bundle.js"></script>
<!-- Flot Charts Plugin Js -->
<script src="~/Areas/AreaAdmin/AdminContent/plugins/flot-charts/jquery.flot.js"></script>
<script src="~/Areas/AreaAdmin/AdminContent/plugins/flot-charts/jquery.flot.resize.js"></script>
<script src="~/Areas/AreaAdmin/AdminContent/plugins/flot-charts/jquery.flot.pie.js"></script>
<script src="~/Areas/AreaAdmin/AdminContent/plugins/flot-charts/jquery.flot.categories.js"></script>
<script src="~/Areas/AreaAdmin/AdminContent/plugins/flot-charts/jquery.flot.time.js"></script>
<!-- Sparkline Chart Plugin Js -->
<script src="~/Areas/AreaAdmin/AdminContent/plugins/jquery-sparkline/jquery.sparkline.js"></script>
<!-- Custom Js -->
<script src="~/Areas/AreaAdmin/AdminContent/js/admin.js"></script>
<script src="~/Areas/AreaAdmin/AdminContent/js/pages/index.js"></script>
<!-- Demo Js -->
<script src="~/Areas/AreaAdmin/AdminContent/js/demo.js"></script>
<!-- Datatable js -->
